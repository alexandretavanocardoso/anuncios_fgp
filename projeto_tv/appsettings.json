{
  "ConnectionStrings": {
    "VanSyncEnterpriseDb": "Server=localhost;Database=AnuncioDB;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True;Integrated Security=SSPI;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Migration": {
    "Vansync": {
      "inicial": "dotnet ef migrations add InitialCreate",
      "update": "dotnet ef database update"
    }
  },
  "AllowedHosts": "*"
}
